<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="C:\Users\Administrator\.m2\repository\com\h2database\h2\1.3.176\h2-1.3.176.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>    
            <property name="suppressDate" value="true"/>    
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->    
            <property name="suppressAllComments" value="true"/>    
        </commentGenerator> 
		<jdbcConnection driverClass="org.h2.Driver"
			connectionURL="jdbc:h2:file:~/.h2/uaap-server;AUTO_SERVER=TRUE;DB_CLOSE_DELAY=-1;DB_CLOSE_ON_EXIT=FALSE"
			userId="sa"
			password="">
		</jdbcConnection>

		<javaTypeResolver >
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- generate Model -->
		<javaModelGenerator targetPackage="com.cj.uaap.entity" targetProject="c:\mybatis-generated\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		

		<!-- generate xml -->
		<sqlMapGenerator targetPackage="com.cj.uaap.dao"  targetProject="c:\mybatis-generated\resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		
		<!-- generate Mapper -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.cj.uaap.dao"  targetProject="c:\mybatis-generated\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		

		<table schema="" tableName="sys_user" domainObjectName="User" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
		<table schema="" tableName="sys_project" domainObjectName="Leavemessage"></table>

	</context>
</generatorConfiguration>
